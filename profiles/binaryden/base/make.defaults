# portage options
EMERGE_DEFAULT_OPTS="--quiet-build=y --read-news=y"
FEATURES="splitdebug clean-logs"
PORTAGE_TMPDIR="/tmp"
PORT_LOGDIR="/var/log/portage"

# USE flags
USE="acl acpi bzip2 -gnome gdbm -gtk -gtk3 ipv6 -kde nls ncurses -qt3support -qt4 -qt5 systemd udev udisks unicode vim-syntax -webkit -X"

L10N="-* en-US en"
PYTHON_SINGLE_TARGET="-* python3_6"
PYTHON_TARGETS="-* python2_7 python3_6"
RUBY_TARGETS="-* ruby22"

